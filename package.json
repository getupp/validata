{
  "name": "validata",
  "version": "2.1.12",
  "description": "Type safe data validation and sanitization",
  "keywords": [
    "typescript",
    "type-safe",
    "typesafe",
    "typed",
    "validate",
    "validator",
    "validation",
    "clean",
    "cleanse",
    "sanitise",
    "sanitize",
    "data validator",
    "data clean",
    "data cleanse",
    "object validator",
    "request validator"
  ],
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:gregbacchus/validata.git"
  },
  "scripts": {
    "lint": "cross-env ./node_modules/.bin/eslint src/ --ext .ts,.tsx",
    "test": "cross-env ./node_modules/.bin/jest --verbose --coverage --runInBand",
    "build": "cross-env rm -rf dist && cross-env ./node_modules/.bin/tsc --outDir dist --sourceMap",
    "prepublishOnly": "npm run build && npm version patch",
    "postpublish": "git push"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5",
    "ulid": "^2.3.0",
    "validator": "^13.1.1"
  },
  "dependencies": {
    "@types/luxon": "^1.24.1",
    "luxon": "^1.24.1"
  }
}
